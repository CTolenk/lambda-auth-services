  "description": "",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.678.0",
    "@aws-sdk/lib-dynamodb": "^3.678.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.149",
    "@vitest/coverage-v8": "^2.1.4",
    "@types/node": "^24.0.1",
    "esbuild": "^0.25.5",
    "typescript": "^5.8.3",
    "vitest": "^2.1.4"
  }
}
