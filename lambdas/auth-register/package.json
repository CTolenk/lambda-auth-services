{
  "name": "lambda-auth-register",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "build": "esbuild src/handler.ts --bundle --platform=node --target=node20 --sourcemap --outfile=dist/index.js",
    "zip": "rm -f function.zip && zip -j function.zip dist/index.js",
    "start": "npm run build && node --enable-source-maps dist/index.js",
    "start:debug": "npm run build && node --inspect-brk=9229 --enable-source-maps dist/index.js"
  },
