{
  "name": "lambda-auth-login",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "npm run build:tests && node --test $(find dist-tests -name '*.test.js')",
    "build:tests": "tsc --project tsconfig.tests.json && node ../shared/scripts/link-shared-tests.cjs dist-tests",
    "build": "esbuild src/handler.ts --bundle --platform=node --target=node20 --external:aws-sdk --sourcemap --outfile=dist/index.js",
    "zip": "zip -j function.zip dist/index.js",
    "start": "npm run build && node --enable-source-maps dist/index.js",
    "start:debug": "npm run build && node --inspect-brk=9229 --enable-source-maps dist/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "aws-sdk": "^2.1692.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.149",
    "@types/node": "^24.0.1",
    "esbuild": "^0.25.5",
    "typescript": "^5.8.3"
  }
}
